  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Run Server",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["o2_server.server:app", "--reload"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "Python: Run main.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run ppo_training.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/ppo_training.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run data_collector.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/data_collector.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run data_analyzer.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/data_analyzer.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run scenario_analyzer.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/scenario_analyzer.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run redumper.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/redumper.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run test.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run redumper_solutions.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/redumper_solutions.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Debug Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": ["debug-test"],
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Run bpmn_model_splitter.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/bpmn_model_splitter.py",
      "cwd": "${workspaceFolder}/examples",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Data Collector (Proximal Policy Optimization, BPI Challenge 2012, MID)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/data_collector.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--name",
        "bpi_challenge_2012_mid",
        "--active-scenarios",
        "BPI_Challenge_2012",
        "--agents",
        "Proximal Policy Optimization",
        "--number-of-cases",
        "1000",
        "--duration-fn",
        "1 / (1 + (log(size) / log(100)))",
        "--max-batch-size",
        "100",
        "--max-iterations",
        "1500",
        "--dump-interval",
        "250",
        "--max-non-improving-actions",
        "1250",
        "--max-threads",
        "24",
        "--log-level",
        "DEBUG",
        "--log-file",
        "./logs/proximal_policy_optimization_bpi_challenge_2012_mid_2025-02-14T02:01:54.408628.log",
        "--log-to-tensor-board",
        "--no-archive-tensorboard-logs"
      ]
    },

    {
      "name": "Python: Run production_simulated_annealing_easy.sh",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/data_collector.py",
      "cwd": "${workspaceFolder}/",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--name",
        "production_easy",
        "--active-scenarios",
        "production",
        "--agents",
        "Simulated Annealing",
        "--number-of-cases",
        "1000",
        "--duration-fn",
        "1/size",
        "--sa-cooling-factor",
        "0.95",
        "--sa-initial-temperature",
        "auto",
        "--max-batch-size",
        "50",
        "--max-iterations",
        "1001",
        "--dump-interval",
        "2500",
        "--max-non-improving-actions",
        "500",
        "--max-threads",
        "24",
        "--max-number-of-actions-to-select",
        "24",
        "--log-level",
        "DEBUG",
        "--log-file",
        "./logs/simulated_annealing_production_easy.log",
        "--log-to-tensor-board",
        "--no-archive-tensorboard-logs"
      ]
    },

    {
      "name": "Python: Run proximal_policy_optimization_bpi_challenge_2012_hard.sh",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/o2_evaluation/data_collector.py",
      "cwd": "${workspaceFolder}/",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--name",
        "bpi_challenge_2012_hard",
        "--active-scenarios",
        "BPI_Challenge_2012",
        "--agents",
        "Proximal Policy Optimization",
        "--number-of-cases",
        "1000",
        "--duration-fn",
        "1 * size",
        "--sa-cooling-factor",
        "auto",
        "--sa-initial-temperature",
        "auto",
        "--max-batch-size",
        "50",
        "--max-iterations",
        "10001",
        "--dump-interval",
        "1000",
        "--max-non-improving-actions",
        "40",
        "--max-threads",
        "24",
        "--max-number-of-actions-to-select",
        "24",
        "--log-level",
        "DEBUG",
        "--log-file",
        "./logs/proximal_policy_optimization_bpi_challenge_2012_hard.log",
        "--log-to-tensor-board",
        "--no-archive-tensorboard-logs"
      ]
    }
  ]
}
